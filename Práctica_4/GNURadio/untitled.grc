- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "\n\nimport numpy as np\nfrom gnuradio import gr\n\nclass blk(gr.sync_block):\
      \  \n    \"\"\"This block is a Complex Exponential (CE) Voltage-Controlled Oscillator\
      \ (VCO) or Baseband VCO. \n    It works by generating a complex exponential\
      \ signal with amplitude and phase modulation.\n    \"\"\"\n\n    def __init__(self,):\
      \  \n        # Initialize the sync block with the specified input and output\
      \ signal types\n        gr.sync_block.__init__(\n            self,\n       \
      \     name='VCO Complex',  # Name of the block\n            in_sig=[np.float32,\
      \ np.float32],  # Input: two float32 arrays (amplitude and phase)\n        \
      \    out_sig=[np.complex64]  # Output: one complex64 array (modulated signal\
      \ in complex form)\n        )\n        \n    def work(self, input_items, output_items):\n\
      \        # Get the amplitude (A) and phase (Q) from the input items\n      \
      \  A = input_items[0]  # Amplitude input signal\n        Q = input_items[1]\
      \  # Phase input signal\n        Sec = output_items[0]  # Output signal array\
      \ (modulated complex signal)\n\n        \n        # Generate the complex exponential\
      \ output signal using the formula:\n        # y = A * exp(j*Q)\n        Sec[:]\
      \ = A * np.exp(1j * Q)  # complex exponential signal\n        \n        \n \
      \       return len(Sec)"
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('VCO Complex', 'blk', [], [('0', 'float', 1), ('1', 'float', 1)],
      [('0', 'complex', 1)], 'This block is a Complex Exponential (CE) Voltage-Controlled
      Oscillator (VCO) or Baseband VCO. \n    It works by generating a complex exponential
      signal with amplitude and phase modulation.\n    ', [])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [832, 608.0]
    rotation: 0
    state: enabled
